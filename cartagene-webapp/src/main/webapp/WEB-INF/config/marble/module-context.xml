<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
  <import resource="classpath:META-INF/spring/marble-core/context.xml" />

  <bean class="org.obiba.onyx.marble.engine.MarbleModule">
    <property name="stages">
      <list>
        <bean class="org.obiba.onyx.engine.Stage">
          <property name="name" value="CON" />
          <property name="module" value="marble" />
        </bean>
      </list>
    </property>
  </bean>

</beans>