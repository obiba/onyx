        </rangeCheck>
        <parameterSpreadCheck>
          <targetParameter reference="sta1_height2" />
          <parameter reference="sta1_height1" />
          <offset>1</offset>
        </parameterSpreadCheck>
      </integrityChecks>
    </output>

    <computedOutput>
      <code>Average_Height_Measurement</code>
      <measurementUnit>cm</measurementUnit>
      <dataType>DECIMAL</dataType>
