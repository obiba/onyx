          <parameter reference="waist_size2" />
          <offset>1</offset>
        </parameterSpreadCheck>
      </integrityChecks>
    </output>

    <computedOutput id="waist_size_avg">
      <code>CALC_AVG_WAIST_SIZE</code>
      <measurementUnit>cm</measurementUnit>
      <dataType>INTEGER</dataType>
      <algorithm>AVERAGE</algorithm>
      <captureMethod>COMPUTED</captureMethod>
      <outputs>
        <output reference="waist_size1" />
        <output reference="waist_size2" />
