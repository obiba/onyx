<?xml version="1.0" encoding="UTF-8"?>
<jnlp
    spec="$jnlpspec"
    codebase="${codebaseUrl}">
  <information>
    <title>$project.name</title>
    <vendor>Obiba</vendor>
#if($offlineAllowed)
    <offline-allowed/>

#end
  </information>
#if($allPermissions)
  <security>
     <all-permissions/>
  </security>
#end
  <resources>
    <j2se version="$j2seVersion"/>
## Make sure the nativelib resource is defined before the others. This is required due to http://jira.codehaus.org/browse/MWEBSTART-8
     <nativelib href="lib/rxtx-lib-2.1.7-r2-win32.jar"/>
     $dependencies
  </resources>
  <application-desc main-class="${mainClass}">
    <argument><![CDATA[__config__]]></argument>
  </application-desc>

</jnlp>

