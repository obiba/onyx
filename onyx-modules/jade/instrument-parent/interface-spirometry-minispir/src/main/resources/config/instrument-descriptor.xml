      </contraIndication>

      <contraIndication>
        <code>AbdominalThoracicEyeSurgery</code>
        <type>ASKED</type>
      </contraIndication>

      <contraIndication>
        <code>ChestInfection</code>
        <type>ASKED</type>
      </contraIndication>

      <contraIndication>
        <code>Tuberculosis</code>
        <type>ASKED</type>
      </contraIndication>

      <contraIndication>
        <code>DetachedRetina</code>
        <type>ASKED</type>
      </contraIndication>

      <contraIndication>
        <code>Pneumothorax</code>
        <type>ASKED</type>
      </contraIndication>

      <contraIndication>
        <code>Stoma</code>
