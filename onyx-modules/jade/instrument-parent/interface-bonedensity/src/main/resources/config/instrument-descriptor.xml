<?xml version="1.0" encoding="ISO-8859-1"?>
<linked-list>

  <!-- Instruments -->
  <!-- spirometer1 -->
  <instrument id="bone1">
    <name>Bone Density 1</name>
    <vendor>GE Healthcare</vendor>
    <model>Achilles Express</model>
    <serialNumber>1234567890</serialNumber>
    <barcode>CAG0210</barcode>    
    <status>ACTIVE</status>
    <instrumentType>
      <name>BON</name>
    </instrumentType>
    <instrumentParameters>
      <input reference="id"/>
      <input reference="last_name"/>
      <input reference="first_name"/>
      <input reference="birth_date"/>
      <input reference="gender"/>
      <input reference="foot_scanned"/>

      <output reference="assessment"/>
      <output reference="fxrisk"/>
      <output reference="total"/>
      <output reference="tscore"/>
      <output reference="agematched"/>
      <output reference="percentnormal"/>
      <output reference="sidescanned"/>
      <output reference="stiffnessindex"/>
      <output reference="id"/>
      <output reference="SOS"/>
      <output reference="BUA"/>
      <output reference="achillesbitmap"/>
    </instrumentParameters>
  </instrument>
  
  <participantPropertySource id="id_source">
    <property>barcode</property>
  </participantPropertySource>
  <input id="id">
    <name>id</name>
    <description>Participant Barcode</description>
    <dataType>TEXT</dataType>
    <captureMethod>AUTOMATIC</captureMethod>
    <inputSource reference="id_source" />
    <instrument reference="bone1" />
  </input>  
  
  <participantPropertySource id="last_name_source">
      <property>lastName</property>
  </participantPropertySource>
  <input id="last_name">
    <name>last_name</name>
    <description>Participant Last Name</description>
    <dataType>TEXT</dataType>
    <captureMethod>AUTOMATIC</captureMethod>
    <inputSource reference="last_name_source" />
    <instrument reference="bone1" />
  </input>
  
  <participantPropertySource id="first_name_source">
      <property>firstName</property>
  </participantPropertySource> 
  <input id="first_name">
    <name>first_name</name>
    <description>Participant First Name</description>
    <dataType>TEXT</dataType>
    <captureMethod>AUTOMATIC</captureMethod>
    <inputSource reference="first_name_source" />
    <instrument reference="bone1" />
  </input>  
  
  <participantPropertySource id="birth_date_source">
      <property>birthDate</property>
  </participantPropertySource>
  <input id="birth_date">
    <name>birth_date</name>
    <description>Participant Date of Birth</description>
    <dataType>DATE</dataType>
    <captureMethod>AUTOMATIC</captureMethod>
    <inputSource reference="birth_date_source" />
    <instrument reference="bone1" />
  </input>
  
  <participantPropertySource id="gender_source">
      <property>gender</property>
  </participantPropertySource>  
  <input id="gender">
    <name>gender</name>
    <description>Participant Gender</description>
    <dataType>TEXT</dataType>
    <captureMethod>AUTOMATIC</captureMethod>
    <inputSource reference="gender_source" />
    <instrument reference="bone1" />
  </input>
    
  <operatorSource id="foot_scanned_source"/> 
  <input id="foot_scanned">
    <name>foot_scanned</name>
    <description>Foot Scanned</description>
    <dataType>TEXT</dataType>
    <captureMethod>MANUAL</captureMethod>
    <inputSource reference="foot_scanned_source" />
    <instrument reference="bone1" />
  </input> 

  <output id="assessment">
    <name>Assessment</name>
    <description>Assessment</description>
    <dataType>DECIMAL</dataType>
    <captureMethod>AUTOMATIC</captureMethod>
    <measurementUnit></measurementUnit>
    <instrument reference="bone1" />
  </output> 
  <output id="fxrisk">
    <name>Fracture Risk</name>
    <description>Fracture Risk</description>
    <dataType>DECIMAL</dataType>
    <captureMethod>AUTOMATIC</captureMethod>
    <measurementUnit></measurementUnit>
    <instrument reference="bone1" />
  </output>  
  <output id="total">
    <name>Stiffness Index Result</name>
    <description>Stiffness Index Result</description>
    <dataType>DECIMAL</dataType>
    <captureMethod>AUTOMATIC</captureMethod>
    <measurementUnit></measurementUnit>
    <instrument reference="bone1" />
  </output>  
  <output id="tscore">
    <name>T-Score</name>
    <description>T-Score</description>
    <dataType>DECIMAL</dataType>
    <captureMethod>AUTOMATIC</captureMethod>
    <measurementUnit></measurementUnit>
    <instrument reference="bone1" />
  </output>   
  <output id="zscore">
    <name>Z-Score</name>
    <description>Z-Score</description>
    <dataType>DECIMAL</dataType>
    <captureMethod>AUTOMATIC</captureMethod>
    <measurementUnit></measurementUnit>
    <instrument reference="bone1" />
  </output> 
  <output id="percentnormal">
    <name>% Young Adult</name>
    <description>% Young Adult</description>
    <dataType>DECIMAL</dataType>
    <captureMethod>AUTOMATIC</captureMethod>
    <measurementUnit>%</measurementUnit>
    <instrument reference="bone1" />
  </output> 
  <output id="agematched">
    <name>% Age Matched</name>
    <description>% Age Matched</description>
    <dataType>DECIMAL</dataType>
    <captureMethod>AUTOMATIC</captureMethod>
    <measurementUnit>%</measurementUnit>
    <instrument reference="bone1" />
  </output>     
  <output id="achillesbitmap">
    <name>Stiffness Index graph</name>
    <description>Stiffness Index graph</description>
    <dataType>DATA</dataType>
    <captureMethod>AUTOMATIC</captureMethod>
    <measurementUnit></measurementUnit>
    <instrument reference="bone1" />
  </output>      
  <output id="stiffnessindex">
    <name>Stiffness Index</name>
    <description>Stiffness Index</description>
    <dataType>DECIMAL</dataType>
    <captureMethod>AUTOMATIC</captureMethod>
    <measurementUnit></measurementUnit>
    <instrument reference="bone1" />
  </output>        
  <output id="achillesbitmap2">
    <name>Ultrasound Graphic</name>
    <description>Ultrasound Graphic</description>
    <dataType>DATA</dataType>
    <captureMethod>AUTOMATIC</captureMethod>
    <measurementUnit></measurementUnit>
    <instrument reference="bone1" />
  </output> 
  <output id="sos">
    <name>Speed of Ultrasound</name>
    <description>Speed of Ultrasound</description>
    <dataType>DECIMAL</dataType>
    <captureMethod>AUTOMATIC</captureMethod>
    <measurementUnit></measurementUnit>
    <instrument reference="bone1" />
  </output>  
  <output id="bua">
    <name>Broadband Ultrasound Attenuation</name>
    <description>Broadband Ultrasound Attenuation</description>
    <dataType>DECIMAL</dataType>
    <captureMethod>AUTOMATIC</captureMethod>
    <measurementUnit></measurementUnit>
    <instrument reference="bone1" />
  </output>    
  <output id="foot_scanned">
    <name>Foot Scanned</name>
    <description>Foot Scanned</description>
    <dataType>TEXT</dataType>
    <captureMethod>AUTOMATIC</captureMethod>
    <measurementUnit></measurementUnit>
    <instrument reference="bone1" />
  </output>    
  <output id="appversion">
    <name>Achilles Software Version</name>
    <description>Achilles Software Version</description>
    <dataType>TEXT</dataType>
    <captureMethod>AUTOMATIC</captureMethod>
    <measurementUnit></measurementUnit>
    <instrument reference="bone1" />
  </output>      
  <output id="roi_x">
    <name>Region of Intersection X coordinate</name>
    <description>Region of Intersection X coordinate</description>
    <dataType>INTEGER</dataType>
    <captureMethod>AUTOMATIC</captureMethod>
    <measurementUnit></measurementUnit>
    <instrument reference="bone1" />
  </output>
  <output id="roi_y">
    <name>Region of Intersection Y coordinate</name>
    <description>Region of Intersection Y coordinate</description>
