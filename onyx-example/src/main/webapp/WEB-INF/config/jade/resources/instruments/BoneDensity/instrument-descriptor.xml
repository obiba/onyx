      <captureMethod>AUTOMATIC</captureMethod>
      <measurementUnit/>
    </output>

    <output>
      <vendorName>tscore</vendorName>
      <code>RES_T-SCORE</code>
      <dataType>DECIMAL</dataType>
      <captureMethod>AUTOMATIC</captureMethod>
      <measurementUnit/>
    </output>

    <output>
      <vendorName>zscore</vendorName>
      <code>RES_Z-SCORE</code>
      <dataType>DECIMAL</dataType>
      <captureMethod>AUTOMATIC</captureMethod>
      <measurementUnit/>
    </output>

    <output>
