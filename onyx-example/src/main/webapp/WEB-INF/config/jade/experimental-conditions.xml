<?xml version="1.0" encoding="UTF-8"?>
<list>

  <experimentalConditionLog>
    <name>RoomCharacteristics</name>
    <attribute>
      <name>Temperature</name>
      <type>INTEGER</type>
    </attribute>
    <attribute>
      <name>RelativeHumidity</name>
      <type>INTEGER</type>
      <unit>%</unit>
    </attribute>
  </experimentalConditionLog>

  <instrumentCalibration>
    <name>TanitaCalibration</name>
    <instructions>
      <string>TanitaCalibration.Instruction1</string>
      <string>TanitaCalibration.Instruction2</string>
      <string>TanitaCalibration.Instruction3</string>
      <string>TanitaCalibration.Instruction4</string>
    </instructions>
    <instrumentType>Impedance418</instrumentType>
    <attribute>
      <name>20kg</name>
      <type>INTEGER</type>
    </attribute>
    <attribute>
      <name>40kg</name>
      <type>INTEGER</type>
    </attribute>
    <attribute>
      <name>ChoiceExample</name>
      <type>TEXT</type>
      <allowedValues>
        <string>Choice1</string>
        <string>Choice2</string>
        <string>Choice3</string>
      </allowedValues>
    </attribute>
  </instrumentCalibration>

  <experimentalConditionLog>
    <name>LocationCharacteristics</name>
    <attribute>
      <name>MetresAboveSeaLevel</name>
      <type>INTEGER</type>
    </attribute>
  </experimentalConditionLog>
  
  <experimentalConditionLog>
    <name>WeatherCharacteristics</name>
    <instructions>
      <string>Weather.Instruction1</string>
      <string>Weather.Instruction2</string>
    </instructions>
    <attribute>
      <name>PossibilityOfPrecipitation</name>
      <type>DECIMAL</type>
    </attribute>
  </experimentalConditionLog>

</list>
